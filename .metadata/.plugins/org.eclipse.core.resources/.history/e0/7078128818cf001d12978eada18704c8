package com.xworkz.oopsprogramming;

public class Acer extends Laptop {
	String hardDisk;
	byte ram;
	
	public Acer(String hardDisk, byte ram) {
		this.hardDisk=hardDisk;
		this.ram=ram;
		System.out.println(this.hardDisk);
		System.out.println(this.ram);
	}
	
	public void goodUsage() {
		System.out.println("Have good usage");	
	}
	
	public void camera() {
		System.out.println("Good quality camera");
	}

}
